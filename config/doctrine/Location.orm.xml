      <generator strategy="IDENTITY"/>
    </id>
    <field name="codeMunicipality" type="integer" column="code_municipality" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nameMunicipality" type="string" column="name_municipality" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codeIneMunicipality" type="integer" column="code_ine_municipality" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codeNuts4" type="integer" column="code_nuts4" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nameNuts4" type="string" column="name_nuts4" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
